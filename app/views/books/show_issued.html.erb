<div class="page-header">
  <h1>Issued Books</h1>
</div>

<% @books.each do |book| %>
  <h5><%= book.title %> - <%= book.author %> </h5> 
  <h6> Issued to <%=book.user.name%> 
  <%= link_to 'Return', book_return_to_library_path(book.id), method: :put, class: "btn"%>
  <%= link_to 'Renew', book_renew_duration_path(book.id), method: :put, class: "btn"%></h6>
<% end %>
